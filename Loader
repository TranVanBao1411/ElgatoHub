local ID = game.PlaceId
local baseURL = "https://raw.githubusercontent.com/meobeo8/elgato/a/"
local Players = game:GetService("Players")
local plr = Players.LocalPlayer

function GetGame()
    if ID == 78681874345659 then
        return "ErrorZone.lua"
    elseif ID == 86639052909924 then
        return "VersePiece.lua"
    elseif ID == 111347281646457 then
        return "MoodengFruit.lua"
    elseif ID == 95295765150201 then
        return "RockFruit.lua"
    elseif ID == 10450270085 or ID == 16379688837 or ID == 119359147980471 then 
        return "JujutsuInfinite.lua"
    else
        plr:Kick("Ramdom kick for fun")
        return nil
    end
end

local gameScript = GetGame()

if gameScript then
    loadstring(game:HttpGet(baseURL .. gameScript))()
end

spawn(function()
    while wait(1) do
        pcall(function()
            for _, v in pairs(game:GetService("Lighting"):GetChildren()) do
                if v.Name == "Blur" then
                    v:Destroy()
                end
            end
        end)
    end
end)

for _, v in next, getconnections(plr.Idled) do
    v:Disable()
end

local VirtualUser = game:GetService("VirtualUser")
local status = getgenv().afk_toggle
if status == nil then
    getgenv().afk_toggle = false
end

if not plr then
    error("Failed to get LocalPlayer reference")
end

plr.Idled:Connect(function()
    if not getgenv().afk_toggle then return end
    pcall(function()
        VirtualUser:CaptureController()
        VirtualUser:ClickButton2(Vector2.new())
    end)
end)

spawn(function()
    while wait(0.1) do
        pcall(function()
            setfpscap(math.random(60, 90))
        end)
    end
end)
